# Exemple de fichier .env
# Copier ce fichier en .env et remplir vos vraies valeurs
# NE PAS committer .env dans Git

# Perplexity AI
PERPLEXITY_API_KEY=pplx-xxxxxxxxxxxxxxxxxxxxx

# Notion API
NOTION_TOKEN=ntn_xxxxxxxxxxxxxxxxxxxxx
NOTION_PARENT_PAGE_ID=xxxxxxxxxxxxxxxxxxxxx

# Email de notification
NOTIFICATION_EMAIL=votre.email@example.com

# Gmail API (OAuth2)
# Contenu JSON de votre fichier credentials.json (Google Cloud Console)
GOOGLE_CREDENTIALS_JSON={"installed":{"client_id":"...","client_secret":"...",...}}
# Contenu JSON de votre fichier token.json (généré après autorisation OAuth2)
GOOGLE_OAUTH_TOKEN_JSON={"token":"...","refresh_token":"...","client_id":"...",...}
